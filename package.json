{
  "name": "stvle",
  "version": "0.0.0",
  "description": "Style variant utility",
  "keywords": [
    "style",
    "variant",
    "variants",
    "utility",
    "typescript",
    "framework-agnostic"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest run",
    "build": "vite build",
    "release": "pnpm run test && pnpm run build && release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamrdevs/stvle.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamrdevs/stvle/issues"
  },
  "devDependencies": {
    "release-it": "^16.1.5",
    "@types/node": "^20.6.3",
    "typescript": "^5.2.2",
    "vite-plugin-dts": "^3.5.4",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  }
}
